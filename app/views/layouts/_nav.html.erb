<div class='topnav'>
  <%= link_to "Home", root_path %>
  <% if !user_signed_in? %>
    <%= link_to "Sign Up", signup_path %>
    <%= link_to "Sign In", login_path %>
  <% else %>
    <%= link_to "User Profile", user_path(current_user.id) %>
    <%= link_to "Log Out", destroy_user_session_path, method: "delete" %>
  <% end %>
</div>
